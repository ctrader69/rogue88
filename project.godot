config_version=5

[application]

config/name="rogue88"
run/main_scene="res://scenes/game.tscn"
config/features=PackedStringArray("4.3")
boot_splash/bg_color=Color(0, 0, 0, 1)
config/icon="res://assets/images/icon.png"

[autoload]

Items="*res://Items.gd"
EventBus="*res://EventBus.gd"
Base="*res://Base.gd"
Mobs="*res://mobs.gd"
FontPico8="*res://fonts/FontPico8.gd"
Events="*res://Events.gd"
Utils="*res://utils.gd"
BeehaveGlobalMetrics="*res://addons/beehave/metrics/beehave_global_metrics.gd"
BeehaveGlobalDebugger="*res://addons/beehave/debug/global_debugger.gd"
OBJ="*res://obj.gd"

[display]

window/size/viewport_width=896
window/size/viewport_height=896
window/size/resizable=false
window/stretch/mode="canvas_items"
window/stretch/scale=7.0

[global]

mous=false
pixel=false

[gui]

common/snap_controls_to_pixels=false
common/drop_mouse_on_gui_input_disabled=true

